#include<stdio.h>


int prog4()
{
	unsigned int num ;

	printf("Please enter the number : \n");
	scanf("%d", &num);
	

	printf("Binary Value before toggle : \n");
	dec2bin(num);	
	printf("\n");

	printf("Bianry Value after toggle even bits : \n");
	even_bit_toggle(num);
	printf("\n");	
	
	printf("Binary Value after toggle odd bits : \n");
	odd_bit_toggle(num);
	printf("\n");
}
