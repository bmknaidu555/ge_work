merge_sort : OBJECT/iaprint.o OBJECT/iascanf.o OBJECT/main.o OBJECT/merge_sort.o 
	gcc -Wall -o merge_sort OBJECT/iaprint.o OBJECT/iascanf.o OBJECT/main.o OBJECT/merge_sort.o 
OBJECT/iaprint.o : SOURCE/iaprint.c
		gcc -Wall -c -o OBJECT/iaprint.o SOURCE/iaprint.c
OBJECT/iascanf.o : SOURCE/iascanf.c
		gcc -Wall -c -o OBJECT/iascanf.o SOURCE/iascanf.c
OBJECT/main.o : SOURCE/main.c
		gcc -Wall -c -o OBJECT/main.o SOURCE/main.c
OBJECT/merge_sort.o : SOURCE/merge_sort.c
		gcc -Wall -c -o OBJECT/merge_sort.o SOURCE/merge_sort.c
