bubble_sort : OBJECT/bubble_sort.o OBJECT/iaprint.o OBJECT/iascanf.o OBJECT/main.o 
	gcc -Wall -o bubble_sort OBJECT/bubble_sort.o OBJECT/iaprint.o OBJECT/iascanf.o OBJECT/main.o 
OBJECT/bubble_sort.o : SOURCE/bubble_sort.c
		gcc -Wall -c -o OBJECT/bubble_sort.o SOURCE/bubble_sort.c
OBJECT/iaprint.o : SOURCE/iaprint.c
		gcc -Wall -c -o OBJECT/iaprint.o SOURCE/iaprint.c
OBJECT/iascanf.o : SOURCE/iascanf.c
		gcc -Wall -c -o OBJECT/iascanf.o SOURCE/iascanf.c
OBJECT/main.o : SOURCE/main.c
		gcc -Wall -c -o OBJECT/main.o SOURCE/main.c
